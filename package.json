{
	"name": "@neovici/cosmoz-utils",
	"version": "3.1.4",
	"description": "Date, money and template management functions commonly needed in Cosmoz views.",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-utils#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-utils/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-utils.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici Development",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "es-dev-server",
		"test": "karma start --coverage",
		"test:watch": "karma start --auto-watch=true --single-run=false",
		"test:update-snapshots": "karma start --update-snapshots",
		"test:prune-snapshots": "karma start --prune-snapshots",
		"test:compatibility": "karma start --compatibility all --auto-watch=true --single-run=false"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@formatjs/intl-pluralrules": "^1.5.2",
		"@formatjs/intl-relativetimeformat": "^4.5.9",
		"@neovici/eslint-config": "^1.2.0",
		"@neovici/github-actions-eslint-annotator": "^0.4.0",
		"@open-wc/testing": "^2.5.4",
		"@open-wc/testing-karma": "^3.3.0",
		"@polymer/polymer": "^3.3.1",
		"@semantic-release/changelog": "^5.0.0",
		"@semantic-release/git": "^9.0.0",
		"babel-eslint": "^10.1.0",
		"deepmerge": "^4.2.2",
		"es-dev-server": "^1.42.0",
		"eslint": "^6.8.0",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-mocha": "^6.3.0",
		"husky": "^4.2.3",
		"karma": "^4.4.1",
		"karma-firefox-launcher": "^1.3.0",
		"karma-sauce-launcher": "^2.0.2",
		"lit-html": "^1.1.2",
		"semantic-release": "^17.0.4"
	}
}
